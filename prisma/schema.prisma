// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    user_id			Int			@default(autoincrement()) @id
	family_name		String
	first_name		String
	middle_name		String?
	login			String		@unique
	password		String
	
}
